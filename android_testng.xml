<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Android : W31-OnBoarding">
	<listeners>
		<listener class-name="com.cs.arris.Listeners.TestListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
	</listeners>
	<parameter name="platform" value="Android" />
	<parameter name="device" value="Android" />
	<parameter name="signup_data" value="testdata/registered_users.json" />
	
	<test name="Enter_Invalid_OTP_Test">
		<classes>
			<class name="com.cs.arris.Tests.TC03_Verify_Invalid_OTP_Test" />
		</classes>
	</test>
	
	<test name="EC_0002_1002_Disconnect_Wifi_On_Mobile_Test">
		<classes>
			<class
				name="com.cs.arris.Tests.TC02_EC_0002_1002_Disconnect_Wifi_On_Mobile_Test" />
		</classes>
	</test> 
	
	<test name="EC_0001_1201_Disconnect_Bluetooth_On_Mobile_Test">
		<classes>
			<class
				name="com.cs.arris.Tests.TC01_EC_0001_1201_Disconnect_Bluetooth_On_Mobile_Test" />
		</classes>
	</test>
	
	<test name="Signin_And_Onboard_Test">
		<classes>
			<class
				name="com.cs.arris.Tests.TC51_Signin_And_Onboard_Test" />
		</classes>
	</test> 

</suite>



